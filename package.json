{
  "name": "ac-config-react",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "assets": "capacitor-assets generate --iconBackgroundColor '#f4f5f8' --splashBackgroundColor '#f4f5f8' --iconBackgroundColorDark '#222428' --splashBackgroundColorDark '#222428'",
    "build": "npm run trapeze && npm run build:web && cap sync",
    "build:web": "tsc && vite build",
    "clean": "git checkout android/variables.gradle android/app/src/main/AndroidManifest.xml ios/App/App/Info.plist",
    "config": "(export $(cat .env | xargs); npm run clean; node scripts/buildConfig.js ; npm run trapeze)",
    "dev": "vite",
    "lint": "eslint",
    "prepare": "husky",
    "preview": "vite preview",
    "test": "vitest run",
    "test:cov": "vitest run --coverage",
    "test:dev": "vitest",
    "test:e2e": "cypress run",
    "trapeze": "(export $(cat .env | xargs) && trapeze run -y config.yaml)"
  },
  "dependencies": {
    "@capacitor/android": "^6.0.0",
    "@capacitor/app": "^6.0.0",
    "@capacitor/core": "^6.0.0",
    "@capacitor/haptics": "^6.0.0",
    "@capacitor/ios": "^6.0.0",
    "@capacitor/keyboard": "^6.0.0",
    "@capacitor/preferences": "^6.0.0",
    "@capacitor/status-bar": "^6.0.0",
    "@ionic-enterprise/auth": "6.0.0",
    "@ionic/react": "~8.1.1",
    "@ionic/react-router": "~8.1.1",
    "@types/react-router": "^5.1.20",
    "@types/react-router-dom": "^5.3.3",
    "ionicons": "^7.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^5.3.4",
    "react-router-dom": "^5.3.4"
  },
  "devDependencies": {
    "@capacitor/assets": "~3.0.5",
    "@capacitor/cli": "^6.0.0",
    "@ionic/prettier-config": "~4.0.0",
    "@testing-library/jest-dom": "~6.4.5",
    "@testing-library/react": "~15.0.7",
    "@testing-library/user-event": "^14.4.3",
    "@trapezedev/configure": "~7.0.10",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@vitejs/plugin-legacy": "~5.4.0",
    "@vitejs/plugin-react": "^4.0.1",
    "@vitest/coverage-v8": "~1.6.0",
    "cypress": "^13.3.3",
    "eslint": "^8.35.0",
    "eslint-plugin-react": "^7.32.2",
    "husky": "~9.0.11",
    "jsdom": "~24.0.0",
    "lint-staged": "~15.2.2",
    "prettier": "~3.2.5",
    "typescript": "^5.1.6",
    "vite": "~5.2.11",
    "vitest": "~1.6.0"
  },
  "description": "An Ionic project"
}
