{
  "name": "ac-config-react",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "assets": "capacitor-assets generate --iconBackgroundColor '#f4f5f8' --splashBackgroundColor '#f4f5f8' --iconBackgroundColorDark '#222428' --splashBackgroundColorDark '#222428'",
    "build": "npm run trapeze && npm run build:web && cap sync",
    "build:web": "tsc && vite build",
    "clean": "git checkout android/variables.gradle android/app/src/main/AndroidManifest.xml ios/App/App/Info.plist",
    "config": "(export $(cat .env | xargs); npm run clean; node scripts/buildConfig.js ; npm run trapeze)",
    "dev": "vite",
    "lint": "eslint .",
    "prepare": "husky",
    "preview": "vite preview",
    "test": "vitest run",
    "test:cov": "vitest run --coverage",
    "test:dev": "vitest",
    "test:e2e": "cypress run",
    "trapeze": "(export $(cat .env | xargs) && trapeze run -y config.yaml)"
  },
  "dependencies": {
    "@capacitor/android": "^7.0.0",
    "@capacitor/app": "^7.0.0",
    "@capacitor/core": "^7.0.0",
    "@capacitor/haptics": "^7.0.0",
    "@capacitor/ios": "^7.0.0",
    "@capacitor/keyboard": "^7.0.0",
    "@capacitor/preferences": "^7.0.0",
    "@capacitor/status-bar": "^7.0.0",
    "@ionic-enterprise/auth": "^7.0.1",
    "@ionic/react": "^8.4.1",
    "@ionic/react-router": "^8.4.1",
    "@types/react-router": "^5.1.20",
    "@types/react-router-dom": "^5.3.3",
    "ionicons": "^8.0.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^5.3.4",
    "react-router-dom": "^5.3.4"
  },
  "devDependencies": {
    "@capacitor/assets": "~3.0.5",
    "@capacitor/cli": "^7.0.0",
    "@ionic/prettier-config": "~4.0.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.4.3",
    "@trapezedev/configure": "^7.1.3",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@vitejs/plugin-legacy": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.1",
    "@vitest/coverage-v8": "^3.0.4",
    "cypress": "^14.0.1",
    "eslint": "^9.31.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "jsdom": "^26.0.0",
    "lint-staged": "^16.0.0",
    "prettier": "^3.4.2",
    "typescript": "^5.1.6",
    "typescript-eslint": "^8.18.1",
    "vite": "^6.2.6",
    "vitest": "^3.0.4"
  },
  "description": "An Ionic project"
}
